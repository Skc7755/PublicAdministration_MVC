                        content: "";
                        position: relative;
                        display: block;
                        height: 15px;
                        width: 15px;
                        background: #d2bdff;
                        box-shadow: 0px 0px 10px 3px #0ff;
                        margin-left: -8px;
                        margin-top: -8px;
                        opacity: 0;
                        transition: all 0.1s linear;
                        border-radius: 100%;
                        animation: star 1.3s linear infinite;
